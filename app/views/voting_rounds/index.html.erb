<h1>Listing voting_rounds</h1>

<table>
  <tr>
    <th>Winning song</th>
    <th>Song id 1</th>
    <th>Song id 2</th>
    <th>Song id 3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @voting_rounds.each do |voting_round| %>
  <tr>
    <td><%= voting_round.winning_song_id %></td>
    <td><%= voting_round.song_id_1 %></td>
    <td><%= voting_round.song_id_2 %></td>
    <td><%= voting_round.song_id_3 %></td>
    <td><%= link_to 'Show', voting_round %></td>
    <td><%= link_to 'Edit', edit_voting_round_path(voting_round) %></td>
    <td><%= link_to 'Destroy', voting_round, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

